# npm
alias n="npm"
alias ni="npm install"
alias nis="npm install --save"
alias nid="npm install --save-dev"
alias nig="npm install -g"
alias nr="npm run"
alias nrm="npm remove --save"
alias ns="npm start"
alias nt="npm test"

# git
alias g="git"
alias gs="git status"
alias gca="git commit -am"
alias gb="git branch"
alias geita="git commit --amend --no-edit"
alias geitanois="git commit --amend --no-edit --no-verify"

alias gpl="git pull"
alias gplr="ggpur"

alias gps="git push"
alias ginit="git init"
alias gc="git checkout"
alias gu="git reset HEAD~"

